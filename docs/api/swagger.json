  "paths": {
    "/ReleaseArtifact/upload/{product}/{os}/{architecture}/{version}": {
      "put": {
        "tags": [
          "ReleaseArtifact"
        ],
        "summary": "Uploads a specific release artifact.",
        "parameters": [
          {
            "name": "product",
            "in": "path",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "description": ""
            }
          },
          {
            "name": "os",
            "in": "path",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "description": ""
            }
          },
          {
            "name": "architecture",
            "in": "path",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "description": ""
            }
          },
          {
            "name": "version",
            "in": "path",
            "description": "",
            "required": true,
            "schema": {
              "type": "string",
              "description": ""
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "artifact"
                ],
                "type": "object",
                "properties": {
                  "artifact": {
                    "type": "string",
                    "description": "",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "artifact": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Upload of the artifact was successful."
          },
          "400": {
            "description": "No or invalid body provided (must be a Zip file)."
          },
          "401": {
            "description": "The user is not authorized (wrong credentials or missing auth header)."
          },
          "500": {
            "description": "Internal error."
          }
        },
        "security": [
          {
            "basic": [ ]
          }
        ]
      }
    },
